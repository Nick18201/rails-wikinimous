  <div class="navbar">
      <%= link_to "Wikinimous", articles_path, class: "logo"%>
    <%= link_to "Create Article", new_article_path %>
  </div>
  <div class=row>
    <div class="container">
      <% @articles.each do |article| %>
        <div class="article">
          <h2 class= title> <%= link_to(article.title, article_path(article)) %></h2>
          <p class= content><%= article.content %></p>
        </div>
      <% end %>
    </div>
  </div>
